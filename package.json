  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:mobile": "ng serve --app mobile --aot --p 4200",
    "start:desktop": "ng serve --app desktop --aot --p 4201",
    "build:mobile": "ng build --app mobile --aot --prod --stats-json",
    "build:mobile:watch": "ng build --app mobile --aot --prod --watch",
    "build:desktop": "ng build --app desktop --aot --prod --stats-json",
    "build:desktop:watch": "ng build --app desktop --aot --prod --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "analyze:mobile": "webpack-bundle-analyzer dist-mobile/stats.json",
    "analyze:desktop": "webpack-bundle-analyzer dist-desktop/stats.json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
